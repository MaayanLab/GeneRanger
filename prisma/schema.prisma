generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model gtex_proteomics {
  gene_id            String?
  tissue             String?
  value              Float?
  tissue_specificity Float?
  id                 Int     @id @default(autoincrement())
}

model hpa {
  gene        String?
  gene_name   String?
  tissue      String?
  cell_type   String?
  level       String?
  reliability String?
  id          Int     @id @default(autoincrement())
}

model hpm {
  gene   String?
  tissue String?
  value  Float?
  id     Int     @id @default(autoincrement())
}
